{
  "include": ["./src/**/*.ts"],
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom"],
    "module": "esnext",
    "outDir": "./types",
    "moduleResolution": "Node",
    "rootDir": "./src",
    "declaration": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  }
}
